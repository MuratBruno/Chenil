networks:
  network: {}
services:
  chenil:
    build:
      context: C:\ProjetProg\Chenil
      dockerfile: Chenil/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Chenil
      target: base
    container_name: Chenil
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: chenil:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Chenil.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      network: {}
    ports:
    - published: 8080
      target: 80
    - target: 80
    - published: 10443
      target: 443
    - target: 443
    tty: true
    volumes:
    - C:\ProjetProg\Chenil\Chenil:/app:rw
    - C:\Users\Bruno Murat\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Bruno Murat\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Bruno Murat\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\Bruno Murat\.nuget\packages:/root/.nuget/packages:ro
    - C:\ProjetProg\Chenil:/src:rw
  db:
    build:
      context: C:\ProjetProg\Chenil
      dockerfile: Chenil/Data/Dockerfile
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: CHANGE_THIS_P4ssW0rd!
    image: chenildb
    networks:
      network: {}
    ports:
    - published: 1435
      target: 1433
    volumes:
    - database:/var/opt/mssql:rw
version: '3.4'
volumes:
  database: {}